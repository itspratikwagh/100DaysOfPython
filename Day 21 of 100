print("let's play a multiplication game bitches")
print()
n=int(input("Please enter any number you want to play with: "))
score=0
for i in range(1,11):
  print(i," x ",n)
  ask=int(input("=  "))
  if ask==n*i:
    print("Fuckin ey mate, you are a math wizard!")
    score+=1
  else:
    print("you are a dumb bitch. go join hollywood")
if score==10:
  print("Bang bang you got a perfect score!")
else:
  print("Your score is ",score)
