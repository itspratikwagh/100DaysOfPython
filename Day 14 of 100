from getpass import getpass as input
print("Epic 🪨📄✂️B attle")
print()
p1=input("Player 1, please choose R, S or P")
p2=input("Player 2, please choose R, S or P")
if p1=="R" and p2=="P":
  print("Player 2 wins!!")
elif p1=="R" and p2=="S":
  print("Player 1 wins!!")
elif p1=="P" and p2=="S":
  print("Player 2 wins!!")
elif p1=="P" and p2=="R":
  print("Player 1 wins!!")
elif p1=="S" and p2=="R":
  print("Player 2 wins!!")
elif p1=="S" and p2=="P":
  print("Player 1 wins!!")
elif p1==p2:
  print("It's a TIE!!")
else:
  print("Try again")
